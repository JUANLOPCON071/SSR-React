{
  "name": "helix-eye",
  "version": "'",
  "description": "Helix Eye is an app that consumes the NASA's API to display their records",
  "main": "index.js",
  "author": "edevars <e.devars.dev@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build-client": "webpack --config webpack.config.client.js",
    "build-server": "webpack --config webpack.config.server.js",
    "build": "cross-env NODE_ENV=development npm run build-client && npm run build-server",
    "build-dev": "cross-env NODE_ENV=development npm run build-server && node --inspect dist/index.js",
    "dev": "nodemon --ext ts --exec \"npm run build-dev\"",
    "start": "cross-env NODE_ENV=development npm run build && cross-env NODE_ENV=development node dist/index.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-regular-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "axios": "^1.8.4",
    "dotenv": "^16.5.0",
    "express": "4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "6.22.3",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@swc/core": "^1.3.24",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.1",
    "@types/node": "^22.14.1",
    "@types/react-dom": "^19.1.2",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.30.0",
    "eslint-plugin-react": "^7.31.11",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^3.1.9",
    "swc-loader": "^0.2.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-node-externals": "^3.0.0"
  }
}
